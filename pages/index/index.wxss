/**index.wxss**/
.container {
  height: 100vh;
  display: grid;
  grid-template-columns: repeat(3, 100px);
  grid-auto-rows: min-content;
  gap: 12px;
  padding: 20px;
  background-color: #f8fafc;
  justify-content: center;
  align-content: start;
}

.quotes-button {
  grid-column: 1;
  grid-row: 1;
}

quotes-button {
  grid-column: 1;
  grid-row: 1;
  width: 100%;
  height: 100%;
}